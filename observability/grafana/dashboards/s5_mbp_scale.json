{
  "title": "S5 MBP Scale",
  "uid": "s5-mbp-scale",
  "schemaVersion": 41,
  "version": 3,
  "timezone": "utc",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Quorum Ratio (prod)",
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
      "targets": [
        {
          "refId": "A",
          "expr": "avg(mbp:oracle:quorum_ratio{env=\"prod\"})"
        }
      ]
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Failover time p95 (s)",
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
      "targets": [
        {
          "refId": "A",
          "expr": "avg(mbp:oracle:failover_time_p95_s{env=\"prod\"})"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Replica staleness p95 (s)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
      "targets": [
        {
          "refId": "A",
          "expr": "mbp:oracle:staleness_p95_s{env=\"prod\"}"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "CDC lag p95 (s)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
      "targets": [
        {
          "refId": "A",
          "expr": "mbp:oracle:cdc_lag_p95_s{env=\"prod\"}"
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Oracle divergence (%)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
      "targets": [
        {
          "refId": "A",
          "expr": "mbp:oracle:divergence_pct{env=\"prod\"}"
        }
      ]
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Auto-resolve latency p95 (s)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
      "targets": [
        {
          "refId": "A",
          "expr": "mbp:auto_resolve:latency_p95_s{env=\"prod\"}"
        }
      ]
    }
  ]
}
