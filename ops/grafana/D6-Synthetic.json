{
  "title": "D6 â€” Synthetic Prober",
  "uid": "d6-synthetic",
  "schemaVersion": 40,
  "version": 1,
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Request rate (5m avg)",
      "datasource": null,
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(synthetic_requests_total[5m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "decimals": 2,
          "unit": "req/s"
        },
        "overrides": []
      },
      "options": {
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "text": {}
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Request rate by route",
      "datasource": null,
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (route)(rate(synthetic_requests_total[5m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "showPoints": "never",
            "spanNulls": true
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "single"
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Latency quantiles (p75/p95)",
      "datasource": null,
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.75, sum by (le)(rate(synthetic_latency_seconds_bucket[5m])))",
          "legendFormat": "p75"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.95, sum by (le)(rate(synthetic_latency_seconds_bucket[5m])))",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "showPoints": "never",
            "spanNulls": true
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Success ratio (5m)",
      "datasource": null,
      "targets": [
        {
          "refId": "A",
          "expr": "avg(synthetic_ok_ratio)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "decimals": 2,
          "unit": "percentunit"
        },
        "overrides": []
      },
      "options": {
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "text": {}
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Error ratio (5m)",
      "datasource": null,
      "targets": [
        {
          "refId": "A",
          "expr": "1 - avg(synthetic_ok_ratio)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "decimals": 2,
          "unit": "percentunit"
        },
        "overrides": []
      },
      "options": {
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "text": {}
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Success vs error ratio",
      "datasource": null,
      "targets": [
        {
          "refId": "A",
          "expr": "avg(synthetic_ok_ratio)",
          "legendFormat": "success"
        },
        {
          "refId": "B",
          "expr": "1 - avg(synthetic_ok_ratio)",
          "legendFormat": "error"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "showPoints": "never",
            "spanNulls": true
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Latency exemplars (trace links)",
      "datasource": null,
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, route)(rate(synthetic_latency_seconds_bucket[1m])))",
          "legendFormat": "{{route}} p95",
          "exemplar": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "links": [
            {
              "title": "Open exemplar trace",
              "targetBlank": true,
              "url": "/explore?left=%5B%22now-1h%22,%22now%22,%22Tempo%22,%7B%22queries%22:%5B%7B%22query%22:%22{trace_id=\\\"${__value.labels.trace_id}\\\"}%22%7D%5D%7D%5D"
            }
          ],
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "showPoints": "never",
            "spanNulls": true
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "desc"
        }
      }
    }
  ]
}
